<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/searchLabel" />
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/albumsLabel" />
        <activity
            android:name=".PlayActivity"
            android:label="@string/playLabel" />
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/artistLabel" />
        <activity
            android:name=".GenresActivity"
            android:label="@string/genresLabel" />
    </application>

</manifest>